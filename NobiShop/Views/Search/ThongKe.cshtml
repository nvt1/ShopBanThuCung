@model IEnumerable<NobiShop.Models.ReporInfo>
@{
    ViewBag.Title = "ThongKe";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<h2>THỐNG KÊ SỐ LƯỢNG SẢN PHẨM</h2>
<table border="1">
    <tr style="background-color:burlywood;font-weight:bold;">
        <td>Mã Loài</td>
        
        <td>Tổng số đã bán</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
           
            <td>@item.Sum</td>
        </tr>
    }
</table>

