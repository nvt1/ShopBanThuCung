<div class="contact100-header flex-sb-m">


    <div>
        <button class="btn-show-contact100">
            Contact Us
        </button>
    </div>
</div>

<div class="container-contact100">
    <div class="wrap-contact100">
        <button class="btn-hide-contact100">
            <i class="zmdi zmdi-close"></i>
        </button>

        <div class="contact100-form-title" style="background-image: url(images/bg-02.jpg);">
            <span>Send Mail</span>
        </div>

        <form action="/FileAndEmail/SendMail" name="SendMail" method="post" enctype="multipart/form-data" class="contact100-form validate-form">
            <p>From</p>
            <div class="wrap-input100 validate-input">
                <input name="From" id="name" class="input100" type="text" placeholder="Eg. example@email.com">
            </div>

            <p>To</p>
            <div class="wrap-input100 validate-input">
                <input name="To" id="email" class="input100" type="text" placeholder="Eg. example@email.com">
            </div>

            <p>Chủ đề</p>
            <div class="wrap-input100 validate-input">

                <input name="Subject" id="phone" class="input100" type="text" placeholder=". . . . .">
            </div>

            <p>Nội dung</p>
            <div class="wrap-input100 validate-input">
                <textarea name="Notes" id="message" class="input100" rows=" 4" placeholder="Your comments..."></textarea>
            </div>
            <div class="wrap-input100 validate-input">
                <input name="Attachment" type="file">
            </div>
            <div class="container-contact100-form-btn">
                <button class="contact100-form-btn" type="submit" value="Send">
                    Send Now
                </button>
            </div>
        </form>
    </div>
</div>



